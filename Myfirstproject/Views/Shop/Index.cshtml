            display: flex;
        }

        #kanxi {
            background-color: lightslategray;
            align-content: center;
            display: flex;
        }

        #ballu {
            background-color: lightslategray;
            align-content: center;
            display: flex;
        }

        #suman {
            margin-left: 0%;
            margin-right: 0%;
            s display: flex;
            flex-wrap: wrap;
            justify-content: space-between;
        }

        .product-item {
            margin-left: 5%;
            margin-right: 5%;
            margin-top: 5%;
            /* Adjust the width as per your design */
            text-align: center;
        }

        .product-image {
            position: relative;
            overflow: hidden;
            border-radius: 8px;
            box-shadow: 0 4px 8px rgba(0,0,0,0.1);
        }

        .product-img {
            width: 100%;
            height: auto;
            transition: transform 0.3s ease; /* Smooth transition for hover effect */
        }

            .product-img:hover {
                transform: scale(1.1); /* Increase image size on hover */
            }

        .product-description {
            background-color: #fff;
            padding: 10px;
        }

            .product-description h6 {
                margin-bottom: 5px;
                font-size: 18px;
                font-weight: 700;
            }

            .product-description p {
                font-size: 16px;
                color: #777;
            }

